{
  "$schema": "http://json.schemastore.org/template",
  "author": "Dimo Dimov",
  "classifications": [ "Web", "Blazor", "WebAssembly" ],
  "identity": "Dimodi.Telerik.Blazor.WebAssembly",
  "name": "Telerik Blazor WebAssembly App",
  "shortName": "dimodi-blazorwasm",
  "description": "Blazor WebAssembly App (ASP.NET Core hosted) with Telerik Blazor components installed",
  "sourceName": "TelerikBlazorWasm",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [{
          "choice": "net6.0",
          "description": "Target .NET 6.0"
      },{
        "choice": "net7.0",
        "description": "Target .NET 7.0"
    }],
      "replaces": "{TVAR_NETVERSION}",
      "defaultValue": "net6.0"
    }
  }
}
