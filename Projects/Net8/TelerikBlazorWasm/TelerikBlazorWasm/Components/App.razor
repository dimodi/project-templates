<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    @{ string telerikUiForBlazorVersion = TelerikBlazorWasm.Client.Layout.MainLayout.GetTelerikUiForBlazorVersion(); }
    @*#if (TPARAM_CDN)
    <script src="https://blazor.cdn.telerik.com/blazor/@telerikUiForBlazorVersion/telerik-blazor.min.js" defer></script>
    <link rel="stylesheet" href="https://blazor.cdn.telerik.com/blazor/@telerikUiForBlazorVersion/kendo-theme-{TVAR_THEME}/swatches/{TVAR_THEME}-main.css" />
    @*#if (TPARAM_ICONTYPE == "Font")
    <link rel="stylesheet" href="https://blazor.cdn.telerik.com/blazor/@telerikUiForBlazorVersion/kendo-font-icons/font-icons.css" />
    #endif*@
    #else
    <script src="_content/Telerik.UI.for.Blazor/js/telerik-blazor.js?@telerikUiForBlazorVersion" defer></script>
    <link rel="stylesheet" href="_content/Telerik.UI.for.Blazor/css/kendo-theme-{TVAR_THEME}/all.css?@telerikUiForBlazorVersion" />
    @*#if (TPARAM_ICONTYPE == "Font")
    <link rel="stylesheet" href="_content/Telerik.UI.for.Blazor/css/kendo-font-icons/font-icons.css?@telerikUiForBlazorVersion" />
    #endif*@
    #endif*@
    @*#if (Framework == "net8.0")
    @*#if (TPARAM_RTL)
    <link rel="stylesheet" href="lib/bootstrap/dist/css/bootstrap.rtl.min.css" />
    #else
    <link rel="stylesheet" href="lib/bootstrap/dist/css/bootstrap.min.css" />
    #endif*@
    <link rel="stylesheet" href="app.css" />
    <link rel="stylesheet" href="TelerikBlazorWasm.styles.css" />
    #else
    @*#if (TPARAM_RTL)
    <link rel="stylesheet" href="@Assets["lib/bootstrap/dist/css/bootstrap.rtl.min.css"]" />
    #else
    <link rel="stylesheet" href="@Assets["lib/bootstrap/dist/css/bootstrap.min.css"]" />
    #endif*@
    <link rel="stylesheet" href="@Assets["app.css"]" />
    <link rel="stylesheet" href="@Assets["TelerikBlazorWasm.styles.css"]" />
    <ImportMap />
    #endif*@
    <link rel="icon" type="image/png" href="favicon.png" />
    <HeadOutlet @rendermode="@Interactive{TVAR_RENDERMODE}" />
</head>

@*#if (TPARAM_RTL)
<body dir="rtl">
#else
<body>
#endif*@
    <Routes @rendermode="@Interactive{TVAR_RENDERMODE}" />
    <script src="_framework/blazor.web.js" autostart="false"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function() {
            Blazor.start();
        });
    </script>
</body>

</html>
